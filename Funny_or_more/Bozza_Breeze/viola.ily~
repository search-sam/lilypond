  \set Score.skipBars = ##t
  \key f \major
  \time 4/4
  
  c4\mf c c8-- c-. r4 | %1
  d4-. r8 d8->( d2) | %2
  ees4 ees ees8-- ees-. r4 | %3
  e4-. r8 e8->( e4.) ees8 | %4
  d4 d d8-- d-. r4 | %5
  bes-. r8 bes->( bes2) | %6
  a4\< a a8-- a-. r8 a-> | %7
  r2\! r8 c d des | %8
  r4 r8 a->( a) bes c4 | %9
  bes4 r8 bes( bes4) bes | %10
  bes4 r8 bes( bes4) bes | %11
  e4 e e8-- e-. r4 | %12
  c4 r8 c8->( c4) c | %13
  bes4 r8 bes( bes4) bes | %14
  bes4 r8 bes( bes4) bes | %15
  a4 r8 a( a4) bes | %16
  a4 r8 a->( a) bes c4 | %17
  a4 f a c | %18
  e8 f e f e d4 a8 | %19
  f'8 e e c c4 f | %20
  fes4-. fes2-> fes4 | %21
  bes,-. r8 bes8->( bes4) bes | %22
  bes4 bes2 bes4 | %23
  c4-. r8 c8->( c4) c-- | %24
  bes4-. r8 bes8->( bes4) bes-- | %25
  bes4 bes2 bes4 | %26
  a4-. r8 a->( a4) bes4->\fp( | %27
  bes2.) bes8--\mf a-. | %28
  a4 a g e | %29
}
  
 
